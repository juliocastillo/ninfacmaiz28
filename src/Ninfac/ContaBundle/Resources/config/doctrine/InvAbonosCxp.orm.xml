<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Ninfac\ContaBundle\Entity\InvAbonosCxp" table="inv_abonos_cxp">
    <indexes>
      <index name="IDX_902A5EAA664AF320" columns="id_empresa"/>
      <index name="IDX_902A5EAA96F5D4E9" columns="id_proveedor"/>
      <index name="IDX_902A5EAA995BA0E1" columns="id_banco"/>
      <index name="IDX_902A5EAA4B41C0C4" columns="id_inventario_referencia"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="inv_abonos_cxp_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="fecha" type="datetime" column="fecha" nullable="true"/>
    <field name="monto" type="decimal" column="monto" precision="10" scale="2" nullable="true"/>
    <field name="comentario" type="string" column="comentario" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userAdd" type="integer" column="user_add" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateAdd" type="datetime" column="date_add" nullable="false"/>
    <field name="userMod" type="integer" column="user_mod" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateMod" type="datetime" column="date_mod" nullable="true"/>
    <many-to-one field="idInventarioReferencia" target-entity="InvInventario" fetch="LAZY">
      <join-columns>
        <join-column name="id_inventario_referencia" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idBanco" target-entity="CtlBanco" fetch="LAZY">
      <join-columns>
        <join-column name="id_banco" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idProveedor" target-entity="MntProveedor" fetch="LAZY">
      <join-columns>
        <join-column name="id_proveedor" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEmpresa" target-entity="CtlEmpresa" fetch="LAZY">
      <join-columns>
        <join-column name="id_empresa" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
