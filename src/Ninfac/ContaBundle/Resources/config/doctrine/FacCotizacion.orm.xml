<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Ninfac\ContaBundle\Entity\FacCotizacion" table="fac_cotizacion">
    <indexes>
      <index name="IDX_6564ADEC664AF320" columns="id_empresa"/>
      <index name="IDX_6564ADEC2A813255" columns="id_cliente"/>
      <index name="IDX_6564ADECF09CE00B" columns="id_tipodocumento"/>
      <index name="IDX_6564ADEC890253C7" columns="id_empleado"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="fac_cotizacion_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="numeroDocumento" type="string" column="numero_documento" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fecha" type="datetime" column="fecha" nullable="true"/>
    <field name="fechaExpira" type="date" column="fecha_expira" nullable="true"/>
    <field name="atencion" type="string" column="atencion" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userAdd" type="integer" column="user_add" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateAdd" type="datetime" column="date_add" nullable="false"/>
    <field name="userMod" type="integer" column="user_mod" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateMod" type="datetime" column="date_mod" nullable="true"/>
    <many-to-one field="idEmpleado" target-entity="MntEmpleado" fetch="LAZY">
      <join-columns>
        <join-column name="id_empleado" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idTipodocumento" target-entity="CtlTipodocumento" fetch="LAZY">
      <join-columns>
        <join-column name="id_tipodocumento" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idCliente" target-entity="CtlCliente" fetch="LAZY">
      <join-columns>
        <join-column name="id_cliente" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEmpresa" target-entity="CtlEmpresa" fetch="LAZY">
      <join-columns>
        <join-column name="id_empresa" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
