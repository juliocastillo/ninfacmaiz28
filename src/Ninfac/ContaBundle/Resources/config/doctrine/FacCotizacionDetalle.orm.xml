<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Ninfac\ContaBundle\Entity\FacCotizacionDetalle" table="fac_cotizacion_detalle">
    <indexes>
      <index name="IDX_C4C5AABCE12F6CD4" columns="id_cotizacion"/>
      <index name="IDX_C4C5AABCF760EA80" columns="id_producto"/>
      <index name="IDX_C4C5AABC664AF320" columns="id_empresa"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="fac_cotizacion_detalle_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="descripcion" type="string" column="descripcion" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cantidad" type="decimal" column="cantidad" precision="10" scale="2" nullable="true"/>
    <field name="precio" type="decimal" column="precio" precision="10" scale="4" nullable="true"/>
    <many-to-one field="idEmpresa" target-entity="CtlEmpresa" fetch="LAZY">
      <join-columns>
        <join-column name="id_empresa" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idProducto" target-entity="CtlProducto" fetch="LAZY">
      <join-columns>
        <join-column name="id_producto" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idCotizacion" target-entity="FacCotizacion" fetch="LAZY">
      <join-columns>
        <join-column name="id_cotizacion" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
