<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Ninfac\ContaBundle\Entity\FacFactura" table="fac_factura">
    <indexes>
      <index name="IDX_60ACA1A523FA9FB2" columns="id_bodega"/>
      <index name="IDX_60ACA1A5890253C7" columns="id_empleado"/>
      <index name="IDX_60ACA1A52A813255" columns="id_cliente"/>
      <index name="IDX_60ACA1A5F09CE00B" columns="id_tipodocumento"/>
      <index name="IDX_60ACA1A54D0513AD" columns="id_partidacontable"/>
      <index name="IDX_60ACA1A5E12F6CD4" columns="id_cotizacion"/>
      <index name="IDX_60ACA1A5B1476DEC" columns="id_forma_pago"/>
      <index name="IDX_60ACA1A5E88F3A0E" columns="id_punto_venta"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="fac_factura_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="fecha" type="datetime" column="fecha" nullable="true"/>
    <field name="numeroDocumento" type="string" column="numero_documento" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="gravadas" type="decimal" column="gravadas" precision="10" scale="4" nullable="true"/>
    <field name="exentas" type="decimal" column="exentas" precision="10" scale="4" nullable="true"/>
    <field name="iva" type="decimal" column="iva" precision="10" scale="4" nullable="true"/>
    <field name="retencion" type="decimal" column="retencion" precision="10" scale="4" nullable="true"/>
    <field name="descuento" type="decimal" column="descuento" precision="10" scale="4" nullable="true"/>
    <field name="total" type="decimal" column="total" precision="10" scale="4" nullable="true"/>
    <field name="afecta" type="boolean" column="afecta" nullable="true"/>
    <field name="userAdd" type="integer" column="user_add" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateAdd" type="datetime" column="date_add" nullable="false"/>
    <field name="userMod" type="integer" column="user_mod" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateMod" type="datetime" column="date_mod" nullable="true"/>
    <many-to-one field="idCotizacion" target-entity="FacCotizacion" fetch="LAZY">
      <join-columns>
        <join-column name="id_cotizacion" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idFormaPago" target-entity="CtlFormaPago" fetch="LAZY">
      <join-columns>
        <join-column name="id_forma_pago" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idPuntoVenta" target-entity="CtlPuntoVenta" fetch="LAZY">
      <join-columns>
        <join-column name="id_punto_venta" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idPartidacontable" target-entity="ConPartidacontable" fetch="LAZY">
      <join-columns>
        <join-column name="id_partidacontable" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idTipodocumento" target-entity="CtlTipodocumento" fetch="LAZY">
      <join-columns>
        <join-column name="id_tipodocumento" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEmpleado" target-entity="MntEmpleado" fetch="LAZY">
      <join-columns>
        <join-column name="id_empleado" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idCliente" target-entity="CtlCliente" fetch="LAZY">
      <join-columns>
        <join-column name="id_cliente" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idBodega" target-entity="CtlBodega" fetch="LAZY">
      <join-columns>
        <join-column name="id_bodega" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
