<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Ninfac\ContaBundle\Entity\MntEmpleadoReferenciaLaboral" table="mnt_empleado_referencia_laboral">
    <indexes>
      <index name="IDX_ABB88983890253C7" columns="id_empleado"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="mnt_empleado_referencia_laboral_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="empresa" type="string" column="empresa" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">nombre del banco</option>
      </options>
    </field>
    <field name="cargo" type="string" column="cargo" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="jefe" type="string" column="jefe" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tiempoTrabajo" type="string" column="tiempo_trabajo" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="motivoRetiro" type="string" column="motivo_retiro" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idEmpleado" target-entity="MntEmpleado" fetch="LAZY">
      <join-columns>
        <join-column name="id_empleado" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
