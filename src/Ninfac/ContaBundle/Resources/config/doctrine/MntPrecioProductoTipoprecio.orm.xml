<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Ninfac\ContaBundle\Entity\MntPrecioProductoTipoprecio" table="mnt_precio_producto_tipoprecio">
    <indexes>
      <index name="IDX_AB2F658BF760EA80" columns="id_producto"/>
      <index name="IDX_AB2F658B3FA73721" columns="id_tipoprecio"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="mnt_precio_producto_tipoprecio_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="precio" type="decimal" column="precio" precision="10" scale="4" nullable="false"/>
    <field name="porcentajeDescuento" type="integer" column="porcentaje_descuento" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="precioFijoMinimo" type="decimal" column="precio_fijo_minimo" precision="10" scale="4" nullable="true">
      <options>
        <option name="comment">especifica el precio minimo a que un producto se puede vender a un determinado cliente.</option>
      </options>
    </field>
    <field name="userAdd" type="integer" column="user_add" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateAdd" type="datetime" column="date_add" nullable="false"/>
    <field name="userMod" type="integer" column="user_mod" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateMod" type="datetime" column="date_mod" nullable="true"/>
    <many-to-one field="idTipoprecio" target-entity="CtlTipoprecio" fetch="LAZY">
      <join-columns>
        <join-column name="id_tipoprecio" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idProducto" target-entity="CtlProducto" fetch="LAZY">
      <join-columns>
        <join-column name="id_producto" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
