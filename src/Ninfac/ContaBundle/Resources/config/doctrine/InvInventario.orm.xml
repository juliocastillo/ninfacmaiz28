<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Ninfac\ContaBundle\Entity\InvInventario" table="inv_inventario">
    <indexes>
      <index name="IDX_AE87F435664AF320" columns="id_empresa"/>
      <index name="IDX_AE87F43560AE4CA2" columns="id_bodega_origen"/>
      <index name="IDX_AE87F4354A851723" columns="id_bodega_destino"/>
      <index name="IDX_AE87F4355A6BB32F" columns="id_tipomovimiento"/>
      <index name="IDX_AE87F435C0674963" columns="id_motivo_movimiento"/>
      <index name="IDX_AE87F435CE5B91D8" columns="id_tipodocumento_inventario"/>
      <index name="IDX_AE87F43596F5D4E9" columns="id_proveedor"/>
      <index name="IDX_AE87F4357C1986A7" columns="id_inventario_traslado"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="inv_inventario_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="fechaMovimiento" type="date" column="fecha_movimiento" nullable="true"/>
    <field name="numeroDocumento" type="string" column="numero_documento" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="comentario" type="string" column="comentario" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userAdd" type="integer" column="user_add" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateAdd" type="datetime" column="date_add" nullable="false"/>
    <field name="userMod" type="integer" column="user_mod" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateMod" type="datetime" column="date_mod" nullable="true"/>
    <many-to-one field="idTipodocumentoInventario" target-entity="CtlTipodocumentoInventario" fetch="LAZY">
      <join-columns>
        <join-column name="id_tipodocumento_inventario" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idProveedor" target-entity="MntProveedor" fetch="LAZY">
      <join-columns>
        <join-column name="id_proveedor" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idInventarioTraslado" target-entity="InvInventario" fetch="LAZY">
      <join-columns>
        <join-column name="id_inventario_traslado" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idMotivoMovimiento" target-entity="CtlMotivoMovimiento" fetch="LAZY">
      <join-columns>
        <join-column name="id_motivo_movimiento" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idTipomovimiento" target-entity="CtlTipomovimiento" fetch="LAZY">
      <join-columns>
        <join-column name="id_tipomovimiento" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idBodegaOrigen" target-entity="CtlBodega" fetch="LAZY">
      <join-columns>
        <join-column name="id_bodega_origen" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idBodegaDestino" target-entity="CtlBodega" fetch="LAZY">
      <join-columns>
        <join-column name="id_bodega_destino" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEmpresa" target-entity="CtlEmpresa" fetch="LAZY">
      <join-columns>
        <join-column name="id_empresa" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
