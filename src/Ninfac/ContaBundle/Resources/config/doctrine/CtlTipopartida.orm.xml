<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Ninfac\ContaBundle\Entity\CtlTipopartida" table="ctl_tipopartida">
    <indexes>
      <index name="IDX_3394E802B2B0C0A" columns="id_formapartida"/>
      <index name="IDX_3394E80664AF320" columns="id_empresa"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="ctl_tipopartida_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="anio" type="integer" column="anio" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nombre" type="string" column="nombre" length="40" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idEmpresa" target-entity="CtlEmpresa" fetch="LAZY">
      <join-columns>
        <join-column name="id_empresa" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idFormapartida" target-entity="CtlFormapartida" fetch="LAZY">
      <join-columns>
        <join-column name="id_formapartida" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
