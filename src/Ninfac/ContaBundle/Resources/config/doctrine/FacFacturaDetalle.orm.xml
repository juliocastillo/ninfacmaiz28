<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Ninfac\ContaBundle\Entity\FacFacturaDetalle" table="fac_factura_detalle">
    <indexes>
      <index name="IDX_EC0AA727760979" columns="id_factura"/>
      <index name="IDX_EC0AA73FA73721" columns="id_tipoprecio"/>
      <index name="IDX_EC0AA723FA9FB2" columns="id_bodega"/>
      <index name="IDX_EC0AA74C84185C" columns="id_porcentaje_descuento"/>
      <index name="IDX_EC0AA7F760EA80" columns="id_producto"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="fac_factura_detalle_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="cantidad" type="decimal" column="cantidad" precision="10" scale="2" nullable="true"/>
    <field name="precio" type="decimal" column="precio" precision="10" scale="4" nullable="true"/>
    <field name="precioDescuento" type="decimal" column="precio_descuento" precision="10" scale="4" nullable="true"/>
    <field name="valor" type="decimal" column="valor" precision="10" scale="4" nullable="true"/>
    <field name="varlorDescuento" type="decimal" column="varlor_descuento" precision="10" scale="4" nullable="true"/>
    <field name="gravadas" type="decimal" column="gravadas" precision="10" scale="4" nullable="true"/>
    <field name="exentas" type="decimal" column="exentas" precision="10" scale="4" nullable="true"/>
    <field name="iva" type="decimal" column="iva" precision="10" scale="4" nullable="true"/>
    <field name="total" type="decimal" column="total" precision="10" scale="4" nullable="true"/>
    <field name="costo" type="decimal" column="costo" precision="10" scale="4" nullable="true"/>
    <many-to-one field="idProducto" target-entity="CtlProducto" fetch="LAZY">
      <join-columns>
        <join-column name="id_producto" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idPorcentajeDescuento" target-entity="CtlPorcentajeDescuento" fetch="LAZY">
      <join-columns>
        <join-column name="id_porcentaje_descuento" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idBodega" target-entity="CtlBodega" fetch="LAZY">
      <join-columns>
        <join-column name="id_bodega" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idTipoprecio" target-entity="CtlTipoprecio" fetch="LAZY">
      <join-columns>
        <join-column name="id_tipoprecio" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idFactura" target-entity="FacFactura" fetch="LAZY">
      <join-columns>
        <join-column name="id_factura" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
